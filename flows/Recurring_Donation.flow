    <description>Recurring Donation Flow - Creates NPSP RD records</description>
    <formulas>
        <name>AllocationPercentageofAmount</name>
        <dataType>Number</dataType>
        <expression>({!Opportunity_Allocations.npsp__Amount__c} / {!getOpportunity.Amount}) * 100</expression>
        <scale>4</scale>
    </formulas>
    <formulas>
        <name>AmountOrEMIAmount</name>
        <dataType>Currency</dataType>
        <expression>IF(Text({!getOpportunity.Donation_Type__c}) !=&apos;EMI&apos;,{!getOpportunity.Amount},{!getOpportunity.EMIAmount__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <description>Record Id is for Card Payment Detail Lookup field</description>
        <name>cardPaymentId</name>
        <dataType>String</dataType>
        <expression>IF ({!paymentMethod} = &apos;Card Payment&apos;, {!recordId}, null)</expression>
    </formulas>
    <formulas>
        <description>Record Id is for Direct Debit Detail Lookup field</description>
        <name>directDebitId</name>
        <dataType>String</dataType>
        <expression>IF ({!paymentMethod} =&apos;Direct Debit&apos;, {!recordId},null)</expression>
    </formulas>
    <formulas>
        <description>Extract Day of the month from the Effective Date</description>
        <name>formulaDayOfMonthly</name>
        <dataType>String</dataType>
        <expression>text(DAY({!effectiveDate}))</expression>
    </formulas>
    <formulas>
        <name>ItemAmount</name>
        <dataType>Currency</dataType>
        <expression>IF({!getOpportunity.DonationCode__c} ==&apos;30N&apos; || {!getOpportunity.DonationCode__c} ==&apos;10N&apos; || {!getOpportunity.DonationCode__c} ==&apos;10D&apos;,Null,{!Opportunity_Allocations.npsp__Amount__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Percentage</name>
        <dataType>Number</dataType>
        <expression>IF({!getOpportunity.DonationCode__c} ==&apos;30N&apos; ||  {!getOpportunity.DonationCode__c} ==&apos;10N&apos; || {!getOpportunity.DonationCode__c} ==&apos;10D&apos;,{!AllocationPercentageofAmount},Null)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <description>Recurring Donation Name</description>
        <name>RecurringDonationName</name>
        <dataType>String</dataType>
        <expression>{!getOpportunity.Name}</expression>
    </formulas>
