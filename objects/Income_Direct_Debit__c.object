    <validationRules>
        <fullName>Account_Number_Length_8_digits</fullName>
        <active>true</active>
        <description>Check that the Account Number entered is 8 digits in length</description>
        <errorConditionFormula>(NOT(ISNUMBER(DD_Bank_Account_Number__c )))   ||  NOT( REGEX( DD_Bank_Account_Number__c , &quot;\\d{8}&quot;))</errorConditionFormula>
        <errorMessage>Account Number must contain 8 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_be_New_Instruction</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISNEW(), NOT(  ISPICKVAL( DD_Status__c , &apos;New Instruction&apos;))  )</errorConditionFormula>
        <errorDisplayField>DD_Status__c</errorDisplayField>
        <errorMessage>All New DD must start as New Instruction</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sort_Code_Length_6_digits</fullName>
        <active>true</active>
        <description>Check that the Sort Code entered is 6 digits in length</description>
        <errorConditionFormula>(NOT(ISNUMBER(DD_Bank_Account_Number__c )))   ||  NOT( REGEX(  DD_Bank_Sort_Code__c  , &quot;\\d{6}&quot;))</errorConditionFormula>
        <errorMessage>Sort Code must contain 6 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>must_be_set</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(TEXT( DD_Collection_Day__c ))</errorConditionFormula>
        <errorDisplayField>DD_Collection_Day__c</errorDisplayField>
        <errorMessage>Must select a Collection date</errorMessage>
    </validationRules>
